{
  "name": "ectest-express",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run build && node --env-file=.env dist/main.js",
    "dev": "nodemon --exec 'node -r ts-node/register --env-file=.env' src/main.ts",
    "build": "rm -rf dist/ .tsbuildinfo && tsc -p tsconfig.build.json",
    "test:unit": "vitest",
    "lint": "eslint . --fix",
    "type-check": "tsc",
    "circular-dep-check": "dpdm src/main.ts",
    "migration:generate": "drizzle-kit generate:pg",
    "migration:run": "node -r ts-node/register --env-file=.env scripts/migrate.ts"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.2",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/formidable": "^3.4.5",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "dpdm": "^3.14.0",
    "drizzle-kit": "^0.20.13",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^3.0.3",
    "pg": "^8.11.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "dependencies": {
    "archiver": "^6.0.1",
    "bcrypt": "^5.1.1",
    "connect-redis": "^7.1.1",
    "drizzle-orm": "^0.29.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.0",
    "formidable": "^3.5.1",
    "helmet": "^7.1.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "postgres": "^3.4.3",
    "rate-limit-redis": "^4.2.0",
    "redis": "^4.6.12"
  }
}
